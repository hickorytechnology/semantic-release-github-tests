{
  "version": "0.1.0",
  "license": "MIT",
  "name": "semantic-release-github-tests",
  "author": "Josh Sullivan",
  "main": "dist/index.js",
  "module": "dist/semantic-release-github-tests.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "commit": "git-cz",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@hickorytechnology/semantic-release-github": "^1.0.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@size-limit/preset-small-lib": "^5.0.2",
    "commitizen": "^4.2.4",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "path": "dist/semantic-release-github-tests.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/semantic-release-github-tests.esm.js",
      "limit": "10 KB"
    }
  ]
}